{
    "builds": [
      {
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",  // Ajuste si n√©cessaire
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ecommerce/wsgi.py"
      }
    ]
  }
  
  